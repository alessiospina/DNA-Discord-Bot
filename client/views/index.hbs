            <div class="row ">
              <div class="col-12 grid-margin">
                <div class="card">
                  <div class="card-body">
                    <h4 class="card-title">Order Status</h4>
                    <div class="table-responsive">
                      <table class="table">
                        <thead>
                          <tr>
                            <th>
                              <div class="form-check form-check-muted m-0">
                                <label class="form-check-label">
                                  <input type="checkbox" class="form-check-input">
                                </label>
                              </div>
                            </th>
                            <th> ID </th>
                            <th> COMMAND </th>
                            <th> DESCRIPTION </th>
                            <th> RESPONSE </th>
                            <th> CREATED </th>
                            <th> UPDATED </th>
                          </tr>
                        </thead>
                        <tbody>
                          {{#if commands.length}}
                            {{#each commands}}
                              <tr>
                                <td>{{id}}</td>
                                <td>{{action}}</td>
                                <td>{{description}}</td>
                                <td>{{response}}</td>
                                <td>{{createdAt}}</td>
                                <td>{{updatedAt}}</td>
                              </tr>
                            {{/each}}
                          {{/if}}
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
